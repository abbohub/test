      },
      "voordelen": {
        "keywords": ["voordeel", "plus", "voordelen"],
        "columns": ["naam", "voordelen"],
        "template": "{naam} biedt als voordeel: {voordelen}."
      },
      "annuleringsvoorwaarden": {
        "keywords": ["annuleringsvoorwaarden", "opzeggen", "annuleren"],
        "columns": ["naam", "annuleringsvoorwaarden"],
        "template": "Voor {naam} gelden de annuleringsvoorwaarden: {annuleringsvoorwaarden}."
      },
      "frequentie_bezorging": {
        "keywords": ["frequentie", "bezorging", "levering"],
        "columns": ["naam", "frequentie_bezorging"],
        "template": "De bezorgfrequentie van {naam} is {frequentie_bezorging}."
