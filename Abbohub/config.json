        "template": "Je kiest voor {naam} omdat: {waarom_kiezen}."
      },
      "hoe_werkt_het": {
        "keywords": ["hoe werkt", "werking", "functies"],
        "columns": ["naam", "hoe_werkt_het"],
        "template": "De werking van {naam} is als volgt: {hoe_werkt_het}."
      },
      "past_dit_bij_jou": {
        "keywords": ["past", "geschikt", "compatibel"],
        "columns": ["naam", "past_dit_bij_jou"],
        "template": "Of {naam} bij jou past? {past_dit_bij_jou}."
      },
      "default": {
        "keywords": [],
        "columns": ["naam", "prijs"],
        "template": "Abonnementen: {naam} ({prijs})"
      }
    }
  }
  
